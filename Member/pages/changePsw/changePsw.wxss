/* pages/index/login.wxss */
page{
  background-color:#F0F3F4;
}
page{
  font-family: "Source Sans Pro","Microsoft Yahei", "微软雅黑","Helvetica Neue",Helvetica,Arial,sans-serif !important;
}

input{
  display: block;
  height: 8rpx;
  padding: 12rpx 24rpx;
  font-size: 14px;
  line-height: 1.42857143;
  color: #555;
  background-color: #fff;
  background-image: none;
  border: 1px solid #ccc;
}

label {
  font-weight: normal;
  font-size: 16px;
  display: inline-block;
  max-width: 100%;
  margin-bottom: 10rpx;
}


.top {
  width: 100%;
  height: 102rpx;
  background-color: #339BFA;
  text-align:left;
  font-size: 36rpx;
  position:fixed; 
  z-index: 99;
  top: 0;
  left: 0;
  float: inherit;
}

.top_copy {
  width: 100%;
  height: 102rpx;
  text-align:left;
  font-size: 36rpx;
  clear:both;
}

.goback {
  width: 150rpx;
  height: 102rpx;
  margin:0rpx auto;
  text-align:center;
  line-height:102rpx;
  float:left;
  color:#FFFFFF;
  background-color: #339BFA;
  padding-right: 30rpx;
  padding-left: 30rpx;
}

.other {
  padding: 20rpx 34rpx;
  font-size: 32rpx;
  line-height: 60rpx;
  float: right;
  text-decoration: none;
  background-color: transparent;
  border: 0;
}

.other_image {
    width: 32rpx;
    height: 32rpx;
}

.middle{
  clear:both;
  background-color:white;
  height:642rpx;
  margin: 0;
  padding:30rpx 30rpx 0 40rpx;
}

.row{
  padding:15rpx 30rpx;
}

.dashed{
  border:2rpx dashed #DEE5E7;
  margin-top:20rpx;
  margin-bottom:20rpx;
  width:100%;
}

.footer{
  background-color: #EDF1F2;
  width:100%;
  padding: 30rpx;
  font-size:28rpx;
}

.require{
  color: red;
}

